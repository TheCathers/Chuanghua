* {
    margin: 0;
    padding: 0;
}
.subnav-control{
    width: 1200px;
    margin: auto;
}
li{
    list-style: none;
}
a{
    text-decoration: none;
}
.head{
    height: 42px;
    margin: 30px auto;
}
.logo{
    float: left;
    width: 198px;
    height: 42px;
    margin-left: 330px;
    margin-top: 5px;

}
.nav{
    float: left;
    margin-left:130px;
}
.nav ul li{
    float: left;
    margin: 0 15px;
}
.nav ul li a{
    display: block;
    height: 42px;
    padding: 0 10px;
    font-size: 18px;
    line-height: 42px;
    color: #050505;
}
.banner{
    margin-left: -100px;
}
.nav ul li a:hover {
    border-bottom: 2px solid #00a4ff;
    color: #00a4ff;
}
.serach{
    float: left;
    width: 412px;
    height: 42px;
    background-color: skyblue;
    margin-left: 60px;
}
#src-text{
    float: left;
    width: 345px;
    height: 40px;
    border: 1px solid #00a4ff;
    border-right: 0;
    color: #bfbfbf;
    font-size: 14px;
    padding-left: 15px;
}
.btn-serach{
    float: left;
    width: 50px;
    height: 42px;
    background-color: #00a4ff;
    color: #ffffff;
    border: 0;
    cursor: pointer;
}
.subnav-control{
    height: 421px;
}
.subnav{
    width: 190px;
    height: 421px;
}
.subnav ul li{
    height: 60px;
    line-height: 45px;
    padding: 0 20px;
}
.subnav ul li a{
    font-size: 18px;
    color: #666;
}
.subnav ul li a:hover{
    border-bottom: 2px solid #00a4ff;
    color: #00a4ff;
}
.main{
    position: absolute;
    right: 20%;
    top: 10%;
    width: 1000px;
    height: 80%;
    background-color: #F6F6F6;
    box-shadow: 2px 2px 10px #909090;
}
.title{
    font-size: 36px;
    color: #8499ff;
    text-align: center;
    margin-top: 50px;
}
input[type=password]{
    margin-top: 50px;
    width: 200px;
    height: 20px;
}
.old-password{
    margin-left: 10px;
}
.submit{
    background-color: #A7A257;
    color:white;
    cursor: pointer;
    width: 100px;
    height: 25px;
    margin-top: 90px;
    margin-left: 450px;
}