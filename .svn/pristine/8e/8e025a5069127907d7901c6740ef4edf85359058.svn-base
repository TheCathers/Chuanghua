* {
    margin: 0;
    padding: 0;
}
.w{
    width: 1200px;
    margin: auto;
}
li{
    list-style: none;
}
a{
    text-decoration: none;
}
.head{
    height: 42px;
    margin: 30px auto;
}
.logo{
    float: left;
    width: 198px;
    height: 42px;
   
}
.nav{
    float: left;
    margin-left: 60px;
}
.nav ul li{
    float: left;
    margin: 0 15px;
}
.nav ul li a{
    display: block;
    height: 42px;
    padding: 0 10px;
    font-size: 18px;
    line-height: 42px;
    color: #050505;
    
}
.nav ul li a:hover {
    border-bottom: 2px solid #00a4ff;
    color: #00a4ff;
}
.serach{
    float: left;
    width: 412px;
    height: 42px;
    background-color: skyblue;
    margin-left: 50px;
}
.serach input{
    float: left;
    width: 345px;
    height: 40px;
    border: 1px solid #00a4ff;
    border-right: 0;
    color: #bfbfbf;
    font-size: 14px;
    padding-left: 15px;
}
.serach button{
    float: left;
    width: 50px;
    height: 42px;
    background-color: #00a4ff;
    color: #ffffff;
    border: 0;
}
.banner .w{
    height: 421px;
}
.subnav{
    width: 190px;
    height: 421px;
}
.subnav ul li{
    height: 60px;
    line-height: 45px;
    padding: 0 20px;
}
.subnav ul li a{
    font-size: 18px;
    color: #666;
}
.subnav ul li a:hover{
    border-bottom: 2px solid #00a4ff;
    color: #00a4ff;
}